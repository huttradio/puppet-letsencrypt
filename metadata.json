{
  "name": "huttradio-letsencrypt",
  "version": "0.1.0",
  "author": "huttradio",
  "summary": "Puppet module for generating and installing Let's Encrypt certificates",
  "license": "Apache-2.0",
  "source": "https://github.com/huttradio/puppet-letsencrypt",
  "project_page": "https://github.com/huttradio/puppet-letsencrypt",
  "issues_url": "https://github.com/huttradio/puppet-letsencrypt/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 3.0.0"},
    {"name":"puppetlabs-vcsrepo","version_requirement":">= 1.3.0"}
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0 <2015.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 <5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "15.10",
        "16.04"
      ]
    }
  ],
  "data_provider": null
}
